import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    int a,b,c;
	    double area,s;
	    Scanner sc=new Scanner(System.in);
	    System.out.println("Enter the three sides of triangle:");
	    a=sc.nextInt();
	    b=sc.nextInt();
	    c=sc.nextInt();
	    s=(a+b+c)/2.0;
	    area=Math.sqrt(s*(s-a)*(s-b)*(s-c));
		System.out.println("Area of triangle is: "+area);
	}
}
